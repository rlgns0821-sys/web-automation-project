<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.korea.boardEx.mapper.UserMapper">

  <select id="findByLoginId" resultType="userVO">
	    SELECT
	      Login_ID    AS loginId,
	      LOGIN_NAME  AS loginName,
	      PHONE 	  AS phone	
	    FROM USERS
	    WHERE login_id = #{loginId}
  </select>

</mapper>
